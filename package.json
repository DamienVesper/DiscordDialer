{
    "name": "discord-dialer",
    "version": "1.0.0",
    "description": "A dializer utilizing MicroSIP and Discord.JS",
    "main": "index.js",
    "scripts": {
        "prod": "grunt build-dist && cross-env NODE_ENV=\"prod\" && nodemon index.js",
        "shard": "grunt build-dist && cross-env NODE_ENV=\"prod\" && nodemon shards.js",
        "dev": "grunt build-dist && cross-env NODE_ENV=\"dev\" && grunt dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DamienVesper/Discord-Dialer.git"
    },
    "keywords": [
        "discord",
        "scambaiting",
        "microsip"
    ],
    "author": "DamienVesper",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/DamienVesper/Discord-Dialer/issues"
    },
    "homepage": "https://github.com/DamienVesper/Discord-Dialer#readme",
    "dependencies": {
        "child_process": "^1.0.2",
        "cross-env": "^7.0.2",
        "discord.js": "^11.5.1",
        "dotenv": "^8.2.0",
        "fs": "0.0.1-security",
        "grunt": "^1.3.0",
        "grunt-concurrent": "^3.0.0",
        "grunt-contrib-clean": "^2.0.0",
        "grunt-contrib-copy": "^1.0.0",
        "grunt-contrib-nodemon": "^0.5.4",
        "grunt-contrib-watch": "^1.1.0",
