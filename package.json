{
  "name": "discord-dialer",
  "version": "1.1.0",
  "description": "A dialer utilizing MicroSIP and Discord.JS",
  "main": "index.js",
  "scripts": {
    "dist-test": "cross-env NODE_ENV=\"prod\" && node ./src/index.js",
    "prod": "cross-env NODE_ENV=\"prod\" && pm2 start ./src/index.js",
    "shard": "cross-env NODE_ENV=\"prod\" && pm2 start ./src/shards.js",
    "dev": "cross-env NODE_ENV=\"dev\" && grunt dev",
    "dev:lint": "eslint . --fix --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DamienVesper/Discord-Dialer.git"
  },
  "keywords": [
    "discord",
    "scambaiting",
    "microsip"
  ],
  "author": "DamienVesper",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DamienVesper/Discord-Dialer/issues"
  },
  "homepage": "https://github.com/DamienVesper/Discord-Dialer#readme",
  "dependencies": {
    "cross-env": "^7.0.3",
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0",
    "grunt": "^1.3.0",
    "grunt-concurrent": "^3.0.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-nodemon": "^0.4.2",
    "path": "^0.12.7",
    "pm2": "^4.5.1",
    "robotjs": "^0.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1"
  }
}
